{
  "name": "@kismet_ai/aura",
  "version": "1.0.173",
  "scripts": {
    "clean": "rm -rf dist && rm -rf .cache",
    "build": "tsc -b",
    "test": "echo \"Error: no test specified\" && exit 1",
    "rollup-build-lib": "rollup -c",
    "prepublishOnly": "yarn build && yarn rollup-build-lib && npm version patch --no-git-tag-version",
    "preparepublish": "yarn run upgrade_libraries && tsc -b && yarn build && npm version patch --no-git-tag-version",
    "release": "yarn run preparepublish && (npx npm publish --access public)",
    "prestorybook": "yarn run upgrade_libraries",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "upgrade_libraries": "yarn add @kismet_ai/foundation@latest",
    "cloud_build_storybook": "gcloud builds submit --config '.storybook/cloudbuild.yaml' --project playgroundist",
    "deploy_storybook": "yarn cloud_build_storybook && gcloud run deploy kismet-bifrost-storybook --region us-central1 --image us-docker.pkg.dev/playgroundist/kismet-docker-repo/kismet-bifrost-storybook:latest"
  },
